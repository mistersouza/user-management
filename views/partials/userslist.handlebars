<div class="container py-3">
  {{#if users}}
  <table class="table">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">First</th>
        <th scope="col">Last</th>
        <th scope="col">email</th>
        <th scope="col">Department</th>
        <th class="text-center" scope="col">Visit profile</th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}
      <tr>
        <th scope="row">{{this.id}}</th>
        <td>{{this.first_name}}</td>
        <td>{{this.last_name}}</td>
        <td>{{this.email}}</td>
        <td>{{this.department}}</t>
        <td class="text-center"><a href="/users/{{this.id}}" class="btn btn-light">
          <i class="bi bi-person-square"></i>
        </a></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
  {{/if}}
</div>